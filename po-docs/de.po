#: ../make-fs/virt-make-fs.pod:166
msgid "Choose the output disk image format."
msgstr ""

#. type: textblock
#: ../make-fs/virt-make-fs.pod:168
msgid "The default is C<raw> (raw sparse disk image)."
msgstr ""

#. type: =item
#: ../make-fs/virt-make-fs.pod:170
