msgstr ""

#. type: =head2
#: ../fish/guestfish-actions.pod:3627
msgid "hivex-node-children"
msgstr "hivex-node-children"

#. type: verbatim
#: ../fish/guestfish-actions.pod:3629
#, no-wrap
msgid ""
" hivex-node-children nodeh\n"
"\n"
msgstr ""

#. type: textblock
#: ../fish/guestfish-actions.pod:3631 ../lib/guestfs-actions.pod:5979
