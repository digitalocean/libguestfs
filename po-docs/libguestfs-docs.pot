#: ../examples/guestfs-testing.pod:179
msgid "Shut the guest down and try copying multiple files and directories:"
msgstr ""

#. type: verbatim
#: ../examples/guestfs-testing.pod:181
#, no-wrap
msgid ""
" virt-copy-in -d Guest /home /etc/issue /tmp\n"
"\n"
msgstr ""

