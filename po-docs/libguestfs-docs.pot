#. type: =head1
#: ../examples/guestfs-recipes.pod:245
msgid "Find out which user is using the most space"
msgstr ""

#. type: textblock
#: ../examples/guestfs-recipes.pod:247
msgid ""
"This simple script examines a Linux guest to find out which user is using "
"the most space in their home directory:"
msgstr ""

#. type: verbatim
#: ../examples/guestfs-recipes.pod:250
#, no-wrap
msgid ""
" #!/bin/sh -\n"
" \n"
msgstr ""

#. type: verbatim
#: ../examples/guestfs-recipes.pod:252 ../fish/guestfish.pod:1017
#, no-wrap
msgid ""
" set -e\n"
" \n"
msgstr ""

#. type: verbatim
#: ../examples/guestfs-recipes.pod:254
#, no-wrap
msgid ""
" vm=\"$1\"\n"
" dir=/home\n"
" \n"
msgstr ""

#. type: verbatim
#: ../examples/guestfs-recipes.pod:257
#, no-wrap
msgid ""
" eval $(guestfish --ro -d \"$vm\" -i --listen)\n"
" \n"
msgstr ""

#. type: verbatim
#: ../examples/guestfs-recipes.pod:259
#, no-wrap
msgid ""
" for d in $(guestfish --remote ls \"$dir\"); do\n"
"     echo -n \"$dir/$d\"\n"
"     echo -ne '\\t'\n"
"     guestfish --remote du \"$dir/$d\";\n"
" done | sort -nr -k 2\n"
" \n"
msgstr ""

#. type: verbatim
#: ../examples/guestfs-recipes.pod:265 ../fish/guestfish.pod:974
#, no-wrap
msgid ""
" guestfish --remote exit\n"
"\n"
msgstr ""

