	@$(MKDIR_P) glthread/$(DEPDIR)
	@: > glthread/$(DEPDIR)/$(am__dirstamp)
glthread/lock.lo: glthread/$(am__dirstamp) \
	glthread/$(DEPDIR)/$(am__dirstamp)
glthread/threadlib.lo: glthread/$(am__dirstamp) \
	glthread/$(DEPDIR)/$(am__dirstamp)
